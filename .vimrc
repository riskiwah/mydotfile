"" Encode
set mouse=a
set encoding=utf-8
set fileencoding=utf-8
set fileencodings=utf-8
set ttyfast
set nocompatible

"" Backspace Indent
set backspace=indent,eol,start

"" Hidden
set hidden

set hlsearch
set number
set linebreak
set ruler
set showmatch
set shiftwidth=4
set tabstop=4
set textwidth=100
set smarttab
set expandtab
set nonu
set binary
set noeol
syntax on

"" Idk
set modeline